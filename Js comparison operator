<!DOCTYPE html>
<html>
<head>
  <title>JS Operators</title>
</head>
<body>

  <h1>Check Number</h1>
  <input type="number" id="numInput">
  <button onclick="checkNumber()">Check</button>

  <p id="output"></p>

  <script src="script.js">
  function checkNumber() {
  var num = document.getElementById("numInput").value;

  if (num > 10) {
    document.getElementById("output").innerHTML = num + " is greater than 10.";
  } else if (num < 10) {
    document.getElementById("output").innerHTML = num + " is less than 10.";
  } else {
    document.getElementById("output").innerHTML = num + " is equal to 10.";
  }
}
</script> </body>
</html>
